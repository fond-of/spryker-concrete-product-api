{
  "name": "fond-of-spryker/concrete-product-api",
  "description": "Concrete product api module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "spryker/api": "^0.1.0 || ^0.2.0 || ^0.3.0",
    "spryker/api-query-builder": "^0.1.0",
    "spryker/kernel": "^3.30.0",
    "spryker/product": "^5.0.0 || ^6.0.0",
    "spryker/propel-orm": "^1.2.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-date-time": "^1.0.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "fond-of-codeception/spryker": "dev-master as 1.0.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\":"src/Generated"
    }
  }
}
